
<!DOCTYPE html>
<html>
  <head>
    <title>
        <%= title %>
    </title>
  </head>

  <body>
      <h>Sum of</h>
      <% s = 0 %>  
      
      <% for(let i = 0;i<10;i++) { %>
            <% s = s + i %>.
            <% if ((i%2) == 0) {%>
                <% s1 = s1 + i %>
            <% } else {%>
                 <% s2 = s2 + i %>   
                <% } %>
          <%}%>
    <%= s %> <%= s1 %> <%= s2 %>
  </body>
</html>